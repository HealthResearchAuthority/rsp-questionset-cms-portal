{
    "$schema": "appsettings-schema.json",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "System": "Warning"
            }
        }
    },
    "ConnectionStrings": {
        "umbracoDbDSN": "Server=(localdb)\\MSSQLLocalDB;Database=umbraco1;Integrated Security=true;TrustServerCertificate=true;",
        "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
    },
    "uSync": {
        "Settings": {
            "ImportAtStartup": "Settings",
            "ExportOnSave": "Settings"
        }
    },
    "Umbraco": {
        "CMS": {
            "Global": {
                "Id": "df26b7d9-1ee5-4c92-8c40-4b4a09da7700",
                "SanitizeTinyMce": true
            },
            "Content": {
                "AllowEditInvariantFromNonDefault": true,
                "ContentVersionCleanupPolicy": {
                    "EnableCleanup": true
                }
            },
            "Unattended": {
                "UpgradeUnattended": false
            },
            "Security": {
                "AllowConcurrentLogins": false
            },
            "ModelsBuilder": {
                "ModelsMode": "SourceCodeAuto"
            },
            "WebRouting": {
                "DisableRedirectUrlTracking": true
            },
            "DeliveryApi": {
                "Enabled": true,
                "PublicAccess": true
            }
        }
    }
}