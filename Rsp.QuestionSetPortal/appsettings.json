{
    "$schema": "appsettings-schema.json",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "System": "Warning"
            }
        }
    },
    "ConnectionStrings": {
        "umbracoDbDSN": "Server=(localdb)\\MSSQLLocalDB;Database=umbraco;Integrated Security=true;TrustServerCertificate=true;",
        "ProviderName": "Microsoft.Data.SqlClient"
    },
    "uSync": {
        "Settings": {
            "ImportAtStartup": "None",
            "ExportOnSave": "Settings"
        }
    },
    "Umbraco": {
        "CMS": {
            "Global": {
                "Id": "df26b7d9-1ee5-4c92-8c40-4b4a09da7700",
                "SanitizeTinyMce": true
            },
            "Content": {
                "AllowEditInvariantFromNonDefault": true,
                "ContentVersionCleanupPolicy": {
                    "EnableCleanup": true
                }
            },
            "Unattended": {
                "UpgradeUnattended": false
            },
            "Security": {
                "AllowConcurrentLogins": false,
                "UserPassword": {
                    "RequiredLength": 16,
                    "RequireNonLetterOrDigit": true,
                    "RequireDigit": true,
                    "RequireLowercase": true,
                    "RequireUppercase": true
                }
            },
            "ModelsBuilder": {
                "ModelsMode": "SourceCodeAuto"
            },
            "WebRouting": {
                "DisableRedirectUrlTracking": true
            }
        }
    },
    "AppSettings": {
        "PortalUrl": "",
        "AzureAppConfiguration": {
            "Endpoint": "",
            "IdentityClientID": ""
        },
        "AllowedHosts": "*"
    }
}